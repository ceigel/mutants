<table id="tasks_list" class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Tasks</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% tasks.each do |task| %>
      <tr>
        <td><%= task.name %></td>
        <td class='text-right'>
          <%= table_button_links(task, task.team) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
