<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Mutant name</th>
      <th ></th>
    </tr>
  </thead>

  <tbody>
    <% @team.mutants.each do |mutant| %>
      <tr>
        <td><%= mutant.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= button_link('Add members', edit_team_members_path(@team), 'btn-success') %>
