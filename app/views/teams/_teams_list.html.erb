<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= link_to team.name, team %></td>
        <td class='text-right'>
          <%= table_button_links(team) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

