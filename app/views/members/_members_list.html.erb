<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th><%= action_name %></th>
      <th ></th>
    </tr>
  </thead>

  <%= content_tag :tbody, data: {type: table_type} do %>
    <% mutants.each do |mutant| %>
      <%= render partial: 'members/mutant', locals: {mutant: mutant, link_creator: link_creator} %>
    <% end %>
  <% end %>
</table>
